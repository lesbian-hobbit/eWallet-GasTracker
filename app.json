{
  "expo": {
    "name": "FirebasePractice",
    "slug": "FirebasePractice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-13.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-13.png",
      "backgroundColor": "#2f3c50",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
      
    },
    "permissions": {
      "allowNotifications": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "NOTIFICATIONS"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
